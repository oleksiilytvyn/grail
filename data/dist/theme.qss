        border: none;
        background: none;
        padding: 0;
        margin: 0;
        margin-top: 2px;
        }

    QSearchEdit:disabled {
        background-color: $base;
        color: $container;
        }

    QSearchEdit:hover {
        border: 1px solid $widget-active;
        color: $base-text;
        }

    QSearchEdit:selected {
        background: $base;
        color: $base;
        }


/** @section QTabWidget ******************************************************/

QTabWidget {
    padding: 0px;
    background-color: transparent;
    selection-background-color: $container;
    }
    
    QTabWidget QFrame {
        border: 0;
        padding: 0;
        margin: 0;
        background-color: transparent;
        }
    
    QTabWidget::pane {
        border: 1px solid $base;
        border-radius: 0px;
        padding: 0px;
        margin: 0px;
        background-color: $base;
        }
    
    QTabWidget::pane:selected {
        background-color: $container;
        border: 1px solid $widget-alt;
        }


/** @section QTabBar ********************************************************/

QTabBar {
    qproperty-drawBase: 0;
    border-radius: 0;
    background-color: $base;
    margin: 0px;
    padding: 0px;
    border: 0px;
    }

    QTabBar::close-button {
        border: 0;
        margin: 2px;
        padding: 0;
        image: url(:/qt/close.png);
        }

    QTabBar::close-button:hover {
        image: url(:/qt/close-hover.png);
        }

    QTabBar::close-button:pressed {
        image: url(:/qt/close-pressed.png);
        }

    /* QTabBar::tab - selected ----------------------------------------------- */

    QTabBar::tab:top:selected:disabled,
    QTabBar::tab:bottom:selected:disabled,
    QTabBar::tab:left:selected:disabled,
    QTabBar::tab:right:selected:disabled {
        color: $base-text-alt;
        background-color: $base;
        border: 0px solid transparent;
        }

    /* QTabBar::tab - !selected and disabled ---------------------------------- */

    QTabBar::tab:top:!selected:disabled,
    QTabBar::tab:bottom:!selected:disabled,
    QTabBar::tab:left:!selected:disabled,
    QTabBar::tab:right:!selected:disabled {
        color: $base-text-alt;
        background-color: $base;
        }

    /* QTabBar::tab - selected ----------------------------------------------- */

    QTabBar::tab {
        color: $base-text;
        background-color: $base-alt;
        border: 0px solid transparent;
        margin: 0px;
        padding: 4px 10px;
        min-width: 5px;
        }

    QTabBar::tab:selected {
        color: $container-text;
        background-color: $container;
        }

    QTabBar::tab:!selected:hover {
        background-color: $widget-alt;
        }

    QTabBar::tab:left {
        margin: 0 0 1px 0;
        }

    QTabBar::tab:right {
        margin: 0 0 1px 0;
        }

    QTabBar::tab:top {
        margin: 0 1px 0 0;
        }

    QTabBar::tab:bottom {
        margin: 0 1px 0 0;
        }

    QTabBar QToolButton::right-arrow:enabled {
        image: url(:/qt/right_arrow.png);
        }

    QTabBar QToolButton::left-arrow:enabled {
        image: url(:/qt/left_arrow.png);
        }

    QTabBar QToolButton::right-arrow:disabled {
        image: url(:/qt/right_arrow_disabled.png);
        }

    QTabBar QToolButton::left-arrow:disabled {
        image: url(:/qt/left_arrow_disabled.png);
        }


/** @section QDockWidget *****************************************************/

QDockWidget {
    outline: none;
    background-color: $base;
    border-radius: $radius;
