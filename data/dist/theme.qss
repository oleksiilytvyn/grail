/** @section QSlider *********************************************************/

QSlider {
    background: transparent;
    }

    QSlider:disabled {
        background: transparent;
        }

    QSlider:focus {
        border: none;
        }

QSlider::groove:horizontal {
    background: $container-alt;
    border: 0px solid $container-alt;
    height: 4px;
    margin: 0px;
    border-radius: $radius;
    }

QSlider::sub-page:horizontal {
    background: $widget;
    border: 0px solid $widget;
    height: 4px;
    margin: 0px;
    border-radius: $radius;
    }

QSlider::sub-page:horizontal:disabled {
    background: $widget-alt;
    }

QSlider::handle:horizontal {
    background: $base;
    border: 1px solid $container;
    width: 8px;
    height: 8px;
    margin: -3px 0;
    border-radius: 4px;
    }

QSlider::handle:horizontal:hover {
    background: $widget-active;
    border: 1px solid $widget-active;
    }

QSlider::groove:vertical {
    background: $container-alt;
    border: 0px solid $container-alt;
    width: 4px;
    margin: 0px;
    border-radius: $radius;
    }

QSlider::sub-page:vertical {
    background: $widget;
    border: 0px solid $widget;
    width: 4px;
    margin: 0px;
    border-radius: $radius;
    }

QSlider::sub-page:vertical:disabled {
    background: $container-alt;
    }

QSlider::handle:vertical {
    background: $base;
    border: 1px solid $container;
    width: 8px;
    height: 8px;
    margin: 0 -3px;
    border-radius: 4px;
    }

QSlider::handle:vertical:hover {
    background: $widget-active;
    border: 1px solid $widget-active;
    }


/** @section QLineEdit *******************************************************/

QLineEdit {
    background-color: $base;
    padding-top: 2px;     /* This QLineEdit fix  103, 111 */
    padding-bottom: 2px;  /* This QLineEdit fix  103, 111 */
    padding-left: 4px;
    padding-right: 4px;
    border-style: solid;
    border: 1px solid $base;
    border-radius: $radius;
    color: $base-text;
    }
    
    QLineEdit:disabled {
        background-color: $base;
        color: $container;
        }
    
    QLineEdit:hover {
        border: 1px solid $widget-active;
        color: $base-text;
        }
    
    QLineEdit:selected {
        background: $base;
        color: $base;
        }



/** @section QLineEdit *******************************************************/

QSearchEdit {
    background-color: $base;
    padding-top: 2px;
    padding-bottom: 2px;
    padding-left: 4px;
    padding-right: 18px;
    border-style: solid;
    border: 1px solid $base;
    border-radius: $radius;
    color: $base-text;
    }

