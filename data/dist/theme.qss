/******************************************************************************
 *
 * Grail Style Sheet for Qt widgets and Plugins
 * some code from: https://github.com/ColinDuquesnoy/QDarkStyleSheet
 *
 *****************************************************************************/

@define radius 3px
@define link-color #4fc3f7

@define base #202020
@define base-alt #353535
@define base-text #b5b2b1
@define base-text-alt #5a5a5a

@define container #505050
@define container-alt #5a5a5a
@define container-text #b5b2b1
@define container-text-alt #3c3c3c
@define container-disabled-text #353535

@define widget #353535
@define widget-alt #3c3c3c
@define widget-text #f1f1f1
@define widget-active #ffc907
@define widget-active-text #202020


/** @section QWidget *********************************************************/

QColorDialog,
QFileDialog,
QFontDialog,
QMainWindow,
QDialog {
    background-color: $container;
    border: 0px solid transparent;
    padding: 0px;
    color: $container-text;
    selection-background-color: $container-alt;
    selection-color: $container-text;
    }


/** @section QMainWindow *****************************************************/
/* This adjusts the splitter in the dock widget, not QSplitter               */

QMainWindow::separator {
    background-color: $base;
    border: 0 solid $base;
    spacing: 0;
    padding: 0px;
    }

    QMainWindow::separator:hover {
        background-color: $base-alt;
        border: 0px solid $base;
        }

    QMainWindow::separator:horizontal {
        width: 2px;
        margin-top: 2px;
        margin-bottom: 2px;
        image: url(:/qt/toolbar-v-separator.png);
        }

    QMainWindow::separator:vertical {
        height: 2px;
        margin-left: 2px;
        margin-right: 2px;
        image: url(:/qt/toolbar-h-separator.png);
        }


/** @section QSplitter *******************************************************/

QSplitter {
    background-color: $base;
    spacing: 0;
    padding: 0;
    margin: 0;
    }

    QSplitter::handle {
        background-color: $base;
        }

    QSplitter::handle:active,
    QSplitter::handle:pressed {
        background-color: $widget-active;
        }

    QSplitter::handle:horizontal {
        width: 2px;
        }

    QSplitter::handle:vertical {
        height: 2px;
        }


/** @section QToolTip ********************************************************/

QToolTip {
    background-color: $base;
    border: 1px solid $base-alt;
    border-radius: $radius;
    color: $base-text;
    padding: 0;   /*remove padding, for fix combo box tooltip*/
    opacity: 230; /*reducing transparency to read better*/
    }


/** @section QStatusBar ******************************************************/

QStatusBar {
    background-color: $base;
    color: $base-text;
    border: 1px solid $base;
    }


/** @section QCheckBox *******************************************************/

QCheckBox {
    background-color: $container;
    color: $widget-text;
    spacing: 4px;
    outline: none;
    padding-top: 4px;
    padding-bottom: 4px;
    }

    QCheckBox:focus {
        border: none;
        }

    QCheckBox:disabled,
    QCheckBox QWidget:disabled {
        background-color: $container;
        color: $container-disabled-text;
        }

    QCheckBox::indicator {
        margin-left: 4px;
        width: 16px;
        height: 16px;
        }

    QCheckBox::indicator:unchecked {
        image: url(:/qt/checkbox_unchecked.png);
        }

    QCheckBox::indicator:unchecked:hover,
    QCheckBox::indicator:unchecked:focus,
    QCheckBox::indicator:unchecked:pressed {
        border: none;
        image: url(:/qt/checkbox_unchecked_focus.png);
        }

    QCheckBox::indicator:unchecked:disabled {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }

    QCheckBox::indicator:checked {
        image: url(:/qt/checkbox_checked.png);
        }

    QCheckBox::indicator:checked:hover,
    QCheckBox::indicator:checked:focus,
    QCheckBox::indicator:checked:pressed {
        border: none;
        image: url(:/qt/checkbox_checked_focus.png);
        }

    QCheckBox::indicator:checked:disabled{
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QCheckBox::indicator:indeterminate {
        image: url(:/qt/checkbox_indeterminate.png);
        }

    QCheckBox::indicator:indeterminate:disabled {
        image: url(:/qt/checkbox_indeterminate_disabled.png);
        }

    QCheckBox::indicator:indeterminate:focus,
    QCheckBox::indicator:indeterminate:hover,
    QCheckBox::indicator:indeterminate:pressed {
        image: url(:/qt/checkbox_indeterminate_focus.png);
        }


/** @section QGroupBox *******************************************************/

QGroupBox {
    font-weight: bold;
    border: 1px solid $container;
    border-radius: $radius;
    padding: 4px;
    margin-top: 16px;
    }

    QGroupBox::title {
        subcontrol-origin: margin;
        subcontrol-position: top left;
        left: 3px;
        padding-left: 3px;
        padding-right: 5px;
        padding-top: 8px;
        padding-bottom: 16px;
        }

    QGroupBox::indicator {
        margin-left: 4px;
        width: 16px;
        height: 16px;
        }

    QGroupBox::indicator {
        margin-left: 2px;
        }

    QGroupBox::indicator:unchecked:hover,
    QGroupBox::indicator:unchecked:focus,
    QGroupBox::indicator:unchecked:pressed {
        border: none;
        image: url(:/qt/checkbox_unchecked_focus.png);
        }

    QGroupBox::indicator:checked:hover,
    QGroupBox::indicator:checked:focus,
    QGroupBox::indicator:checked:pressed {
        border: none;
        image: url(:/qt/checkbox_checked_focus.png);
        }

    QGroupBox::indicator:checked:disabled {
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QGroupBox::indicator:unchecked:disabled {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }


/** @section QRadioButton ****************************************************/

QRadioButton {
    background-color: $container;
    color: $container-text;
    spacing: 0;
    padding: 0;
    border: none;
    outline: none;
    }

    QRadioButton:focus {
        border: none;
        }

    QRadioButton:disabled {
        background-color: $container;
        color: $container-disabled-text;
        border: none;
        outline: none;
        }

    QRadioButton QWidget {
        background-color: $base;
        color: $container-text;
        spacing: 0px;
        padding: 0px;
        outline: none;
        border: none;
        }

    QRadioButton::indicator {
        border: none;
        outline: none;
        margin-bottom: 2px;
        width: 25px;
        height: 25px;
        }

    QRadioButton::indicator:unchecked {
        image: url(:/qt/radio_unchecked.png);
        }

    QRadioButton::indicator:unchecked:hover,
    QRadioButton::indicator:unchecked:focus,
    QRadioButton::indicator:unchecked:pressed {
        border: none;
        outline: none;
        image: url(:/qt/radio_unchecked_focus.png);
        }

    QRadioButton::indicator:checked {
        border: none;
        outline: none;
        image: url(:/qt/radio_checked.png);
        }

    QRadioButton::indicator:checked:hover,
    QRadioButton::indicator:checked:focus,
    QRadioButton::indicator:checked:pressed {
        border: none;
        outline: none;
        image: url(:/qt/radio_checked_focus.png);
        }

    QRadioButton::indicator:checked:disabled {
        outline: none;
        image: url(:/qt/radio_checked_disabled.png);
        }

    QRadioButton::indicator:unchecked:disabled {
        image: url(:/qt/radio_unchecked_disabled.png);
        }


/** @section QMenuBar ********************************************************/

QMenuBar {
    background-color: $base;
    padding: 0px;
    border: 0px solid $base;
    color: $base-text;
    }
    
    QMenuBar:focus {
        border: 0px solid $container;
        }
    
    QMenuBar::item {
        background: transparent;
        padding: 4px;
        }
    
    QMenuBar::item:selected {
        padding: 4px;
        background: $base-alt;
        border: 0px solid $base;
        }
    
    QMenuBar::item:pressed {
        padding: 4px;
        border: 0px solid $base;
        background-color: $base-alt;
        color: $base-text;
        }


/** @section QWidget *********************************************************/

@if WINDOWS
QMenu {
    border: 0px solid $container;
    background-color: $base;
    color: $base-text;
    margin: 0px;
    }
    
    QMenu::separator {
        height: 2px;
        background-color: $base-alt;
        color: $container-text;
        padding-left: 4px;
        margin-left: 2px;
        margin-right: 2px;
        }
    
    QMenu::icon {
        margin: 0px;
        padding-left:4px;
        }
    
    QMenu::item {
        padding: 4px 24px 4px 24px;
        border: 1px transparent $container;
        background-color: $base;
        }
    
    QMenu::item:selected {
        color: $widget-text;
        background-color: $base-alt;
        }

    QMenu::item:disabled {
        color: $base-text;
        }

    QMenu::indicator {
        width: 12px;
        height: 12px;
        padding-left: 6px;
        }

    QMenu::indicator:non-exclusive:unchecked {
        image: url(:/qt/checkbox_unchecked.png);
        }
    
    QMenu::indicator:non-exclusive:unchecked:selected {
        image: url(:/qt/checkbox_unchecked_disabled.png);
        }
    
    QMenu::indicator:non-exclusive:checked {
        image: url(:/qt/checkbox_checked.png);
        }
    
    QMenu::indicator:non-exclusive:checked:selected {
        image: url(:/qt/checkbox_checked_disabled.png);
        }

    QMenu::indicator:exclusive:unchecked {
        image: url(:/qt/radio_unchecked.png);
        }
    
    QMenu::indicator:exclusive:unchecked:selected {
        image: url(:/qt/radio_unchecked_disabled.png);
        }
    
    QMenu::indicator:exclusive:checked {
        image: url(:/qt/radio_checked.png);
        }
    
    QMenu::indicator:exclusive:checked:selected {
        image: url(:/qt/radio_checked_disabled.png);
        }
    
    QMenu::right-arrow {
        margin: 5px;
        image: url(:/qt/right_arrow.png)
        }
@end


/** @section QAbstractItemView ***********************************************/


QAbstractItemView {
    background: $container;
    alternate-background-color: $container-alt;
    color: $container-text;
    border: 1px solid $container;
    }

    QAbstractItemView::item {
        background: transparent;
        }

    QAbstractItemView QLineEdit {
        padding: 2px;
        }
    

/** @section QAbstractScrollArea *********************************************/

QAbstractScrollArea {
    background-color: $container;
    border: 0px solid $container;
    padding: 4px;
    color: $container-text;
    }
    
    QAbstractScrollArea:disabled {
        color: $widget-text;
        }


/** @section QScrollArea *****************************************************/

QScrollArea {
    background: $container;
    }

    QScrollArea QWidget QWidget {
        background-color: $container;
        }

    QScrollArea:disabled {
        background: $container-alt;
        }

    QScrollArea QWidget QWidget:disabled {
        background-color: $container-alt;
        }


/** @section QScrollBar *********************************************************/

QScrollBar:horizontal {
    background: transparent;
    border: none;
    height: 8px;
    margin: 4px 4px 0 4px;
    }

    QScrollBar::handle:horizontal {
        background: $base;
        min-width: 20px;
        border-radius: 3px;
        }

    QScrollBar::add-line:horizontal {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        }

    QScrollBar::sub-line:horizontal {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: top;
        subcontrol-origin: margin;
        }

    QScrollBar::up-arrow:horizontal,
    QScrollBar::down-arrow:horizontal {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

    QScrollBar::add-page:horizontal,
    QScrollBar::sub-page:horizontal {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

QScrollBar:vertical {
    background: transparent;
    border: none;
    width: 8px;
    margin: 4px 4px 4px 0;
    }

    QScrollBar::handle:vertical {
        background: $base;
        min-height: 20px;
        border-radius: 3px;
        }

    QScrollBar::add-line:vertical {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: bottom;
        subcontrol-origin: margin;
        }

    QScrollBar::sub-line:vertical {
        background: transparent;
        border: none;
        height: 0;
        width: 0;
        subcontrol-position: top;
        subcontrol-origin: margin;
        }

    QScrollBar::up-arrow:vertical,
    QScrollBar::down-arrow:vertical {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

    QScrollBar::add-page:vertical,
    QScrollBar::sub-page:vertical {
        width: 0;
        height: 0;
        background: none;
        border: none;
        }

/** @section QTextEdit *******************************************************/

QTextEdit {
    background-color: $base;
    color: $container-text;
    border: 1px solid $container;
    }
    
    QTextEdit:hover {
        border: 1px solid $widget-active;
        color: $container-text;
        }
    
    QTextEdit:selected {
        background: $widget-alt;
        color: $container;
        }

    QTextEdit:disabled {
        background: $base;
        color: $container;
        }


/** @section QPlainTextEdit **************************************************/

QPlainTextEdit {
    background-color: $base;
    color: $container-text;
    border-radius: $radius;
    border: 1px solid $container;
    }
    
    QPlainTextEdit:hover {
        border: 1px solid $widget-active;
        color: $container-text;
        }
    
    QPlainTextEdit:selected {
        background: $widget-alt;
        color: $container;
        }

    QPlainTextEdit:disabled {
        background: $base;
        color: $container;
        }

    QPlainTextEdit:disabled:hover {
        border: 1px solid transparent;
        }

/** @section QSizeGrip *******************************************************/

QSizeGrip {
    image: url(:/qt/sizegrip.png);
    width: 12px;
    height: 12px;
    background-color: transparent;
    }


/** @section QStackedWidget **************************************************/

QStackedWidget {
    padding: 0;
    border: 0px solid $base;
    }


/** @section QToolBar ********************************************************/

QToolBar {
    background-color: $base;
    padding: 3px;
    min-height: 24px;
    border-top: 1px solid $base-alt;
    }
    
    QToolBar QToolButton {
        background-color: $base;
        color: $base-text;
        }

    QToolBar QLabel {
        background-color: $base;
        color: $base-text;
        border: 0px solid transparent;
        outline: none;
        }
    
    QToolBar::handle:horizontal {
        width: 6px;
        image: url(:/qt/toolbar-h-move.png);
        }
    
    QToolBar::handle:vertical {
        height: 6px;
        image: url(:/qt/toolbar-v-move.png);
        }
    
    QToolBar::separator:horizontal {
        width: 3px;
        image: url(:/qt/toolbar-h-separator.png);
        }
    
    QToolBar::separator:vertical {
        height: 3px;
        image: url(:/qt/toolbar-v-separator.png);
        }
    
    QToolButton#qt_toolbar_ext_button {
        background: $container;
        border: 0px;
        color: $container-text;
        image: url(:/qt/right_arrow.png);
        }


/** @section QAbstractSpinBox ************************************************/

QAbstractSpinBox {
    background-color: $base;
    border: 1px solid $container;
    color: $container-text;
    padding-top: 2px;     /* This fix  103, 111*/
    padding-bottom: 2px;  /* This fix  103, 111*/
    padding-left: 4px;
    padding-right: 4px;
    border-radius: $radius;
    }
    
    QAbstractSpinBox:up-button {
        background-color: transparent $base;
        subcontrol-origin: border;
        subcontrol-position: top right;
        border-left: 1px solid $container;
        margin: 1px;
        }
    
    QAbstractSpinBox::up-arrow,
    QAbstractSpinBox::up-arrow:disabled,
    QAbstractSpinBox::up-arrow:off {
        image: url(:/qt/up_arrow_disabled.png);
        width: 9px;
        height: 9px;
        }
    
    QAbstractSpinBox::up-arrow:hover {
        image: url(:/qt/up_arrow.png);
        }
    
    QAbstractSpinBox:down-button {
        background-color: transparent $base;
        subcontrol-origin: border;
        subcontrol-position: bottom right;
        border-left: 1px solid $container;
        margin: 1px;
        }
    
    QAbstractSpinBox::down-arrow,
    QAbstractSpinBox::down-arrow:disabled,
    QAbstractSpinBox::down-arrow:off {
        image: url(:/qt/down_arrow_disabled.png);
        width: 9px;
        height: 9px;
        }
    
    QAbstractSpinBox::down-arrow:hover {
        image: url(:/qt/down_arrow.png);
        }
    
    QAbstractSpinBox:hover {
        border: 1px solid $widget-active;
        color: $container-text;
        }
    
    QAbstractSpinBox:selected {
        background: $widget-alt;
        color: $container;
        }

    QAbstractSpinBox:disabled {
        background: $base;
        color: $container;
        }


/** @section QLabel **********************************************************/

QLabel {
    border: 0px solid $container;
    padding: 0px;
    margin: 0px;
    color: $container-text;
    background-color: transparent;
    }
    
    QLabel::disabled {
        color: $container-disabled-text;
        }


/** @section QTextBrowser ****************************************************/

QTextBrowser {
    background-color: transparent;
    border: 0px solid $container;
    color: $container-text;
    border-radius: $radius;
    }
    
    QTextBrowser:disabled {
        background-color: $base;
        border: 1px solid $container;
        color: $widget-text;
        border-radius: $radius;
        }
    
    QTextBrowser:hover,
    QTextBrowser:!hover,
    QTextBrowser::selected,
    QTextBrowser::pressed {
        border: 1px solid $container;
        }


/** @section QGraphicsView ***************************************************/

QGraphicsView {
    background-color: $base;
    border: 1px solid $container;
    color: $container-text;
    border-radius: $radius;
    }
    
    QGraphicsView:disabled {
        background-color: $base;
        border: 1px solid $container;
        color: $widget-text;
        border-radius: $radius;
        }
    
    QGraphicsView:hover,
    QGraphicsView:!hover,
    QGraphicsView::selected,
    QGraphicsView::pressed {
        border: 1px solid $container;
        }



/** @section QCalendarWidget *************************************************/

QCalendarWidget {
    border: 1px solid $container;
    border-radius: $radius;
    }

    QCalendarWidget:disabled {
        background-color: $base;
        color: $container-disabled-text;
        }


/** @section QLCDNumber ******************************************************/

QLCDNumber {
    background-color: $container;
    color: $container-text;
    }

    QLCDNumber:disabled {
        background-color: $container;
        color: $container-disabled-text;
        }


/** @section QProgressBar ****************************************************/

QProgressBar {
    background-color: $widget;
    border: 1px solid $widget;
    color: $widget-text;
    border-radius: $radius;
    text-align: center;
    }
    
    QProgressBar:disabled {
        background-color: $widget;
        border: 1px solid $widget;
        color: $widget-text;
        border-radius: $radius;
        text-align: center;
        }
    
    QProgressBar::chunk {
        background-color: $widget-active;
        color: $widget-active-text;
        border-radius: $radius;
        }
    
    QProgressBar::chunk:disabled {
        background-color: $widget-alt;
        color: $widget-text;
        border-radius: $radius;
        }


/** @section QPushButton *****************************************************/

QPushButton {
    background-color: $widget;
    border: 1px solid $widget;
    color: $widget-text;
    border-radius: $radius;
    padding: 5px 5px;
    outline: none;
    font-weight: normal;
    }
    
    QPushButton:disabled {
        background-color: $container;
        border: 1px solid $widget;
        color: $widget;
        border-radius: $radius;
        padding: 3px;
        }
    
    QPushButton:checked {
        background-color: $widget-active;
        border: 1px solid $widget-active;
        color: $widget-active-text;
        border-radius: $radius;
        padding: 3px;
        outline: none;
        }
    
    QPushButton:checked:disabled {
        background-color: $widget;
        border: 1px solid $widget;
        color: $container;
        border-radius: $radius;
        padding: 3px;
        outline: none;
        }

    QPushButton::menu-indicator {
        subcontrol-origin: padding;
        subcontrol-position: bottom right;
        bottom: 5px;
        }

@if MAC
    QPushButton::menu-indicator {
        bottom: 10px;
        right: 2px;
        }
@end

    QPushButton:pressed {
        background-color: $widget-active;
        border: 1px solid $widget-active;
        color: $widget-active-text;
        }
    
    QPushButton:hover,
    QPushButton:checked:hover {
        background-color: $widget-active;
        border: 1px solid $widget-active;
        color: $widget-active-text;
        }
    
    QPushButton:selected,
    QPushButton:checked:selected {
        background-color: $widget-active;
        border: 1px solid $widget-active;
        color: $widget-active-text;
        }

QDialogButtonBox QPushButton,
QDialogButtonBox QPushButton:disabled {
    padding: 4px 7px;
    }

/** @section QToolButton *****************************************************/

QToolButton {
    background-color: transparent;
    margin: 0px;
    padding: 4px;
    border: 1px solid transparent;
    }

    QToolButton:disabled {
        background-color: $container;
        color: $widget;
        border: 1px solid $widget;
        }

    /* the subcontrols below are used only in the MenuButtonPopup mode */
    
    QToolButton[popupMode="1"] {
        padding: 2px;
        padding-right: 12px;     /* only for MenuButtonPopup */
        border: 1px solid $container;   /* make way for the popup button */
        border-radius: $radius;
    }
    
    /* The subcontrol below is used only in the InstantPopup or DelayedPopup mode */
    
    QToolButton[popupMode="2"] {
        padding: 2px;
        padding-right: 12px;      /* only for InstantPopup */
        border: 1px solid $container;    /* make way for the popup button */
        }
    
    QToolButton::menu-button {
        padding: 2px;
        border-radius: $radius;
        border: 1px solid $container;
        /* 16px width + 4px for border = 20px allocated above */
        width: 16px;
        outline: none;
        }
    
    QToolButton::menu-button:hover,
    QToolButton::menu-button:checked:hover {
        border: 1px solid $widget-active;
        }
    
    QToolButton::menu-indicator {
        image: url(:/qt/down_arrow.png);
        top: -8px;     /* shift it a bit */
        left: -4px;    /* shift it a bit */
        }
    
    QToolButton::menu-arrow {
        image: url(:/qt/down_arrow.png);
        }
    
    QToolButton::menu-arrow:open {
        border: 1px solid $container;
        }


/** @section QCommandLinkButton **********************************************/

QCommandLinkButton {
    background-color: $widget;
    border: 1px solid $widget;
    color: $widget-text;
    border-radius: $radius;
    padding: 0px;
    margin: 0px;
    }

    QCommandLinkButton:hover,
    QCommandLinkButton:active {
        background-color: $widget-active;
        border: 1px solid $widget-active;
        color: $widget-active-text;
        }

    QCommandLinkButton:disabled {
        background-color: transparent;
        border: 1px solid $widget;
        color: $widget;
        }


/** @section QComboBox *******************************************************/

QComboBox {
    border: 1px solid $widget;
    background-color: $widget;
    border-radius: $radius;
    selection-background-color: $widget;
    padding: 4px;
    outline: none;
    color: $widget-text;
    }
    
    QComboBox:disabled {
        background-color: $container;
        color: $widget;
        }
    
    QComboBox:hover,
    QComboBox:on {
        border: 1px solid $widget-active;
        background: $widget-active;
        selection-background-color: $widget-active;
        selection-color: $widget-active-text;
        color: $widget-active-text;
        }

