QListView::indicator:indeterminate:pressed {
    image: url(:/qt/checkbox_indeterminate_focus.png);
    }

QTreeView::indicator:indeterminate,
QListView::indicator:indeterminate {
    image: url(:/qt/checkbox_indeterminate.png);
    }

QListView,
QTreeView,
QTableView,
QColumnView {
